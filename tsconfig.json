{
	"compilerOptions": {
		"target": "es2021",
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "module": "es2020",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "jsx": "preserve",
        "baseUrl": "not_node_modules",
	},
	"include": [
		"src/*.ts",
	]
}